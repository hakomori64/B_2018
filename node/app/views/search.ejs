<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>LIST APP</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <header>
    </header>
    <div class="container">
      <div class="titlearea">
        kensaku moziretu
      </div>
      <div class="index-table-wrapper">
        <ul class="table-body">
          <% recipes.forEach((recipe) => { %>
            <li>
              <table>
                <td>
                  <%= recipe.name %>
                </td> 
                <td>
                  <%= recipe.desc %>
                </td>
              </table>
            </li>
          <% }) %>
        </ul>
      </div>
    </div>
  </body>
</html>
